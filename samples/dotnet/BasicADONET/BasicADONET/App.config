<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Specify a database server ip-address for Data Source. And, you must use Integrated Security=true for -->
  <!-- NosDB OSS. For NosDB Enterprise, you can use Integrated Security=false that means NosDB security and -->
  <!-- then followed by userName= and password= values. -->
  <connectionStrings>
    <add name="NosDbConnection" connectionString="Data Source=127.0.0.1; Port=9950; Database=northwind; Integrated Security=true;" providerName="NosDB.ADO.NETProvider"/>
  </connectionStrings>

  <system.data>
    <DbProviderFactories>
      <add invariant="NosDB.ADO.NETProvider" name="NosDB ADO.NET Provider" description="ADO.NET Provider for NosDB NosQL Database" type="NosDB.ADO.NETProvider.NosProviderFactory, NosDB.ADO.NETProvider, 
                 Version=2.0.0.0, Culture=neutral, PublicKeyToken=8a1e00327893b9ef"/>
    </DbProviderFactories>
  </system.data>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
